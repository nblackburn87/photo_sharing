
<%= form_for @photo, :html => { :multipart => true } do |f| %>
  <div class="form-group">
    <%= f.label :name %></br>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :image, "Photo" %>
    <%= f.file_field :image %>
  </div>
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>
